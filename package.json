{
  "name": "pipe-timer-front",
  "version": "0.0.1",
  "description": "A Time-stackable Pomodoro Timer",
  "productName": "Pipe Timer",
  "author": "Yi Byeongyong <ybyo@yibyeongyong.com>",
  "private": true,
  "scripts": {
    "start:dev": "cross-env ENV_NAME=development quasar dev -m pwa",
    "build:dev": "cross-env ENV_NAME=staging quasar build -m pwa --history",
    "serve:dev": "cross-env ENV_NAME=staging quasar serve ./dist/pwa --hostname 127.0.0.1 --history --https --port 4000 -m pwa -K ./certs/127.0.0.1-key.pem -C ./certs/127.0.0.1-cert.pem",
    "lint": "eslint --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "icon-svg": "fantasticon src/svg-icons -o public/icon-font --normalize -t woff woff2 -g css"
  },
  "dependencies": {
    "@quasar/extras": "^1.15.11",
    "@tanstack/vue-query": "^4.20.5",
    "@vee-validate/zod": "^4.7.3",
    "@vueuse/core": "^9.9.0",
    "axios": "^0.21.4",
    "axios-retry": "^3.3.1",
    "class-transformer": "^0.5.1",
    "dayjs": "^1.11.7",
    "lodash-es": "^4.17.21",
    "pinia": "^2.0.11",
    "pinia-plugin-persistedstate": "^3.0.2",
    "quasar": "^2.11.7",
    "ulid": "^2.3.0",
    "vee-validate": "^4.7.3",
    "vue": "^3.0.0",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6",
    "vuedraggable": "^4.1.0",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@intlify/vite-plugin-vue-i18n": "^3.3.1",
    "@quasar/app-vite": "^1.0.0",
    "@quasar/cli": "^2.0.0",
    "@types/chance": "^1.1.3",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^12.20.21",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "autoprefixer": "^10.4.2",
    "chance": "^1.1.9",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-vue": "^9.0.0",
    "prettier": "^2.5.1",
    "typescript": "^4.5.4",
    "workbox-build": "^6.5.4",
    "workbox-cacheable-response": "^6.5.4",
    "workbox-core": "^6.5.4",
    "workbox-expiration": "^6.5.4",
    "workbox-precaching": "^6.5.4",
    "workbox-routing": "^6.5.4",
    "workbox-strategies": "^6.5.4"
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
