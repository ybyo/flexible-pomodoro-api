{
  "name": "backend",
  "version": "0.0.1",
  "description": "Backend of Pipe Timer",
  "author": "Yi Doyoon",
  "private": true,
  "license": "GPL-3.0",
  "scripts": {
    "prebuild": "rimraf dist",
    "build:staging": "cross-env NODE_ENV=staging nest build",
    "format": "prettier --write \\\"src/**/*.ts\\\" \\\"test/**/*.ts\\\"",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start:compose": "docker compose --profile=local --env-file=../env/.development.env up",
    "start:db": "docker compose --env-file=../env/.development.env up -d mysql-pt redis-pt",
    "start:db:test": "docker compose --env-file=../env/.development.env up -d mysql-test redis-test",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "start:prod:pm2": "pm2 start npm --name \"pipe-timer\" -- run start:production",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --no-cache",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "cross-env TEST=true NODE_ENV=development jest --config test/jest-e2e.json --detectOpenHandles",
    "typeorm": "typeorm-ts-node-commonjs -d src/db/orm.config.ts",
    "migration:create": "typeorm-ts-node-commonjs migration:create src/migrations/table",
    "migration:generate": "npm run typeorm migration:generate src/migrations/table",
    "migration:show": "npm run typeorm migration:show",
    "migration:run": "npm run typeorm migration:run",
    "migration:revert": "npm run typeorm migration:revert"
  },
  "dependencies": {
    "@automapper/classes": "^8.7.7",
    "@automapper/core": "^8.7.7",
    "@automapper/nestjs": "^8.7.7",
    "@nestjs/platform-express": "^9.3.0",
    "@nestjs/common": "^9.3.0",
    "@nestjs/core": "^9.3.0",
    "@nestjs/axios": "^1.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/cqrs": "^9.0.1",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/schedule": "^2.2.0",
    "@nestjs/swagger": "^6.3.0",
    "@nestjs/terminus": "^9.1.2",
    "@nestjs/throttler": "^4.0.0",
    "@nestjs/typeorm": "^9.0.1",
    "@sendgrid/mail": "^7.7.0",
    "@types/chance": "^1.1.3",
    "@types/ejs": "^3.1.1",
    "argon2": "^0.30.2",
    "chance": "^1.1.9",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "connect-redis": "^6.1.3",
    "cookie-parser": "^1.4.6",
    "ejs": "^3.1.8",
    "express-basic-auth": "^1.2.1",
    "express-session": "^1.17.3",
    "helmet": "^6.0.1",
    "identicon.js": "^2.3.3",
    "ioredis": "^5.2.4",
    "joi": "^17.7.0",
    "jsonwebtoken": "^9.0.0",
    "leo-profanity": "^1.7.0",
    "mysql2": "^2.3.3",
    "nest-winston": "^1.8.0",
    "nodemailer": "^6.8.0",
    "passport": "^0.6.0",
    "passport-custom": "^1.1.1",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.10",
    "ulid": "^2.3.0",
    "uuid": "^9.0.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cron": "^2.0.0",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.5",
    "@types/jest": "28.1.8",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/leo-profanity": "^1.5.0",
    "@types/node": "^16.0.0",
    "@types/nodemailer": "^6.4.6",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "jest": "28.1.3",
    "pm2": "^5.2.2",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/../src/$1",
      "^@Helper/(.*)$": "<rootDir>/../src/common/helper/$1"
    }
  }
}
